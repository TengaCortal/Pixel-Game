  <main id="main">

    <h2> Creation of a canva </h2>

    <form action="/canva/create" method="post" role="form" class="php-email-form p-3 ">
        <div class="form-group">
          <input type="name" class="form-control" name="name" id="name" placeholder="Name" required>
        </div>
        <div class="form-group">
          <input type="theme" class="form-control" name="theme" id="theme" placeholder="Theme" required>
        </div>
        <div class="form-group">
            <input type="width" class="form-control" name="width" id="width" placeholder="Width" required>
        </div>
        <div class="form-group">
            <input type="height" class="form-control" name="height" id="height" placeholder="Height" required>
        </div>
    
        <div class="text-center"><button type="submit">Validate</button></div>
    </form>
        
    
  </main><!-- End #main -->

